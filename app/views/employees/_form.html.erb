<%if flash[:errors]%>
<ul>
    <%flash[:errors].each do |error|%>
    <li><%=error%></li>
    <%end%>
</ul>
<%end%>


<%=form_for @employee do |f|%>
    <%=f.label :first_name%>
   <br> <%=f.text_field :first_name%></br>
    <%=f.label :last_name%>
    <br><%=f.text_field :last_name%></br>
    <%=f.label :alias%>
    <br><%=f.text_field :alias%></br>
    <%=f.label :title%>
    <br><%=f.text_field :title%></br>
    <%=f.label :office%>
    <br><%=f.text_field :office%></br>
    <%=f.label :dog_id %>
    <%=f.collection_select :dog_id,Dog.all,:id,:name%>
    
    <br><%=f.submit%></br>
<%end%>

