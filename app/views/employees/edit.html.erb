<h1>Edit an Employee!</h1>

<%= form_for @employee do |e| %>
<%= e.label :first_name %>
<%= e.text_field :first_name %>
<br>
<%= e.label :last_name %>
<%= e.text_field :last_name %>
<br>
<%= e.label :alias %>
<%= e.text_field :alias %>
<br>
<%= e.label :title %>
<%= e.text_field :title %>
<br>
<%= e.label :img_url %>
<%= e.text_field :img_url %>
<br>
<%= e.label :dog_id %>
<%= e.collection_select :dog_id, Dog.all, :id, :name %>
<br>
<%= e.submit %>

<% end %>